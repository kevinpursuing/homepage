html {
    height:100%;
}
body {
    -moz-user-select:none;
    /*火狐*/
    -webkit-user-select:none;
    /*webkit浏览器*/
    -ms-user-select:none;
    /*IE10*/
    -khtml-user-select:none;
    /*早期浏览器*/
    user-select:none;
    background-color:#f9f9f9;
    -webkit-tap-highlight-color:rgba(0,0,0,0);
    height:100%;

    -webkit-overflow-scrolling: touch;
    -webkit-transform: translate3d(0, 0, 0);
}
.head-div {
    box-shadow: 0 0 1rem rgba(40, 57, 81, 0.27);
    height: 1.18rem;
}
.title {
     font-size:0.4rem!important;
 }
.product-title {
    font-size:0.3rem!important;
}
.content-s {
    font-size:0.2rem!important;
}
.img-product {
    width:1.5rem;
    height:1.5rem;
}
.info-white {
    font-size: 0.2rem;
    color:white;
}
.info-gray {
    font-size: 0.2rem;
    color:#C1C1C1;
}
.info-team {
    font-size:0.2rem!important;
    color:#8c8c8c;
    margin-left:auto;
    margin-right:auto;
    margin-bottom: 0;
}
.a-record {
    font-size:0.2rem;
    color:#666666;
    text-decoration:none;
}

.section-content{
    padding: 0.4rem 0.3rem 0 0.3rem
}

.product_box{
    text-align: center;
    margin: 0.2rem auto;
    padding: 0;
    width: 90%;
}

.title_product {
    line-height: 0.6rem;
    text-align: center;
    font-size: 0.4rem;
    font-weight: 300;
    margin-top: 0.39rem;
    font-style: normal;
    font-stretch: normal;
    color: #676a71;
}

.product_text {
    text-align: center;
    font-size: 0.2rem;
    font-weight: 300;
    font-style: normal;
    font-stretch: normal;
    color: #90949e;
    margin-bottom: 5px;
}

.pr_sys{
    line-height: 0.2rem;
    text-align: center;
    margin-top: 39px;
    margin-bottom: 0.2rem;
    font-size: 0.4rem;
    font-weight: 300;
    font-style: normal;
    font-stretch: normal;
    color: #6c6f74;
}

.pr_text{
    line-height: 0.3rem;
    text-align: center;
    font-size: 0.2rem;
    font-weight: 300;
    font-style: normal;
    font-stretch: normal;
    color: #353535;
}
.sys_text{
    line-height: 0.3rem;
    text-align: center;
    font-size: 0.2rem;
    font-weight: 300;
    font-style: normal;
    font-stretch: normal;
    color: #b3b6ba;
}

.partner_title{
    line-height: 0.4rem;
    margin-top: 0.4rem;
    font-size: 0.4rem;
    font-weight: 300;
    font-style: normal;
    font-stretch: normal;
    color: #353535;
}