body {
    background-color: #ebebf1;
    font-family: "Microsoft Yahei", Helvetica, Tahoma, Arial, "Hiragino Sans GB", "Hiragino Sans GB W3",  STXihei, STHeiti, Heiti, SimSun, sans-serif!important;
}

.base_slide {
    position: fixed;
    width: 160px;
    height: 100%;
    text-align: center;
    background-color: #3b424c;
    overflow-x:hidden ;
    overflow-y:auto;
    color: white;
}

.base_logo_div {
    padding-top: 20px;
    width: 100%;
    height: 80px;
    border-bottom: 1px solid #3b3c43
}

.base_create {
    height: 50px;
    line-height: 50px;
    text-align: center;
    border-bottom: 1px solid #3b3c43;
    /*padding-left: 35px;*/
}

.base_create span {
    display: inline-block;
    vertical-align: middle;
    cursor: pointer;
}

.base_create:hover {
    /*background-color: #383a3d;*/
    background-color: #576b95;
}

.base_watch {
    width: 100%;
    height: 60px;
    text-align: left;
    padding-top: 10px;
    padding-bottom: 10px;
    border-bottom: 1px solid #3b3c43
}

.base_watch span {
    /*margin-left: 35px;*/
}

.base_menu {
    width: 160px;
    padding-top: 10px;
    padding-bottom: 10px;
    border-bottom: 1px solid #3b3c43;
}

.base_menu_sub:hover {
    /*background-color: #383a3d;*/
    background-color: #576b95;
}

.active { /*与base_menu_sub配合使用*/
    background-color: #576b95;
}

.base_menu img {

    vertical-align: middle;
}

.base_menu_sub {
    width: 160px;
    height: 36px;
    line-height: 36px;
    cursor: pointer;
    text-align: center;
    /*padding-left: 5px;*/
}

.base_menu_sub span {
    /*margin-left: 30px;*/
    display: inline-block;
    vertical-align: middle;
}

.base_menu_sub_title {
    width: 140px;
    height: 40px;
    line-height: 40px;
    cursor:default ;
    text-align: left;
    padding-left: 27px;
}

.base_menu_sub_title span {
    display: inline-block;
    vertical-align: middle;
    margin-left: 5px;
    color: #a5a7b4;
}

.base_right {
    position: absolute;
    left: 160px;
    right: 0
}

.base_topBar {
    position: fixed;
    height: 50px;
    width: 100%;
    line-height: 50px;
    z-index: 1000;
    background-color: #fff;
    border-bottom: 1px solid #ececec!important;
    /*box-shadow: 0 0 4px 0 rgba(189, 189, 189, 0.5)*/
    padding-right: 10px;
}

.base_exit {
    float: right;
    margin-right: 180px; /*其中有140是因为左边的导航条宽为120px*/
    font-size: 25px;
    cursor: pointer;
    height: 100%;
}

.base_exit span {
    float:left;
    display: inline-block;
    font-size: 16px;
    color: #b8b8b8;
    margin:-2px 0 0 0;
}

.base_explain
{
    float: left;
    margin-left: 20px;
    height: 100%;
}
.base_explain span
{
    display: inline-block;
    font-size: 18px;
    color: #333;
}


.base_content {
    position: relative;
    top: 50px;
    width: 100%;
    padding: 15px;
    background-color: #F9F9F9;
}

.base_mainContent {
    padding: 10px;
    background-color: #fff;
    min-height: 600px;
}

#base_loading {
    position: fixed;
    z-index: 10000;
    display: none;
    background-color: rgba(0, 0, 0, 0.33);
    left: 0;
    top: 0;
    bottom: 0;
    right: 0
}

#login_progressImage {
    position: absolute;
    left: 45%;
    top: 20%;
    /*margin-left: -50px;*/
    margin-top: 150px;
    display: none;
}

/*分页样式*/

.list-page {
    padding: 20px 0;
    text-align: center;
    margin-right: 120px
}

.list-page li {
    display: inline;
}

.list-page a {
    margin: 0 5px;
    padding: 2px 7px;
    background-color: #54bb98;
    /*border-radius: 50% 50%;*/
    color: white;
    /*border-bottom:1px solid #54bb98;*/
    text-decoration: none;
}

.list-page li.active span {
    margin: 0 5px;
    padding: 2px 7px;
    /*background-color:#54bb98;*/
    /*border-radius: 50% 50%;*/
    color: #fff;
    border-bottom: 1px solid #54bb98;
    text-decoration: none;
    background: #2c9de4 !important;
    border-color: #2c9de4 !important;
}

.list-page a:hover {
    background: #408e73;
}

.list-page .current {
    margin: 0 5px;
    padding: 2px 7px;
    background: #f60;
    border: 1px solid #fe8101;
    color: #fff;
}

/*圆形图像*/
.img-circle {
    width: 50px;
    height: 50px;
    vertical-align: middle;
    /*background: url(../../images/default.jpg) center center no-repeat;*/
    /*border: 1px solid #ddd;*/
}

.img-circle-hover {
    cursor: pointer;
}

.img_rect {
    width: 120px;
    height: 60px;
}

/*表格表单*/
.form-control {
    display: inline-block;
    font-size: 12px;
    background-color: #fff!important;
    /*border: 1px solid #e1e6f0!important;*/
}

/*input中placeholder中的字体*/
.form-control::-webkit-input-placeholder {
    color: #b8b8b8;
}

.long {
    /*form-control的长度*/
    width: 300px;
}

.table td {
    text-align: center;
    height: 60px;
    /*line-height: 60px !important;*/
    vertical-align: middle !important; /*设置垂直居中*/
}

.td_left {
    text-align: left !important;
}

.table tr {
    text-align: center;
    height: 60px;
    line-height: 60px !important;
    border-bottom: 1px solid #ddd !important;
}

.table th {
    text-align: center;
    height: 40px;
    vertical-align: middle !important;
}

.th_left {
    text-align: left !important;
}

.table-pointer {
    cursor: pointer;
}

th {
    background-color: #f2f4f5;
    border-bottom: 1px solid #ddd !important;
    border-top: 1px solid #ddd !important;
}

/*按钮样式*/
/* 为了解决bootstrap点击按钮出现蓝色边框的问题*/
.btn:focus,
.btn:active:focus,
.btn.active:focus,
.btn.focus,
.btn:active.focus,
.btn.active.focus {
    outline: none;
}

.btn-default {
    margin-left: 10px;
}

.btn-blue {
    /*background-color: #2c9de4 !important;*/
    background-color: #2e64cb !important;
    border-color: transparent !important;
    color: #fff !important
}

.btn-green {
    background-color: #42d3c3 !important;
    border-color: transparent !important;
    color: #fff !important;
}

.btn-blue:hover, .btn-blue :active, .btn-blue :focus, .btn-blue :visited {
    /*background-color: #2c9de4 !important;*/
    background-color: #2e64cb !important;
    border-color: transparent !important;
    color: #fff !important;
}

.btn-green:hover, .btn-green :active, .btn-green :focus, .btn-blue :visited {
    background-color: #42d3c3 !important;
    border-color: transparent !important;
    color: #fff !important;
}

/*解决谷歌默认填充表单的黄色背景*/
input:-webkit-autofill {
    -webkit-box-shadow: 0 0 0px 1000px white inset;
    border: 1px solid #CCC !important;
}

/*超链接*/
a {
    text-decoration: none !important;
    cursor: pointer !important;
}


.show{
    display: block;
}
.hide{
    display: none;
}

.base_content_title
{
    font:normal bold normal 14px/1.5em 'Microsoft YaHei';
    letter-spacing: -0.3px;
    color: #3e3e3e;
}

.base_slide .base_logo_div img {
    width: 115px;
}
/*头像区*/
.base_avatar
{
    float: right;
    margin-right: 30px;
    height: 100%;
}
.base_avatar img
{
    width: 40px;
    height: 40px;
    float: left;
    margin-top: 3px;
    border-radius: 20px;
    cursor: pointer;
}
.base_avatar span
{
    display: block;
    float: left;
    font:normal normal normal 16px/50px 'Microsoft YaHei';
    margin: -2px 0 0 5px;
    color: #b8b8b8;
    cursor: pointer;
}

td img {
    width: 50px;
    height: 50px;
}

.base_icon
{
    margin-right: 5px;
}

.title_long{
    max-width: 150px !important;
    word-break: break-all !important;
    word-wrap: break-word !important;
}

.operate_long{
    min-width: 150px !important;
}

.operate_long_comment {
    min-width: 180px !important;
}

.status_min_width {
    min-width: 60px;
}

.type_min_width {
    min-width: 50px;
}

.content_long {
    max-width: 180px !important;
    word-break: break-all !important;
    word-wrap: break-word !important;
}