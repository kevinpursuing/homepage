.revenueInformation{margin-top:10px;margin-bottom:10px;width:100%;height:100px;border-width:1px;border-style:none;border-top:1px solid #ECECEC;border-bottom:1px solid #ECECEC}.purchaseTable th,tbody tr{border-bottom:1px solid #ececec!important}.ul-revenue{margin:auto;width:100%;height:100px}.purchaseTable{width:100%}.purchaseTable th{font:normal bold normal 14px/1.5em 'Microsoft YaHei';letter-spacing:-.3px;color:#353535;height:50px;border-top:1px solid #ececec!important;text-align:center}.imgRight,tbody tr{font:normal normal normal 14px/1.5em 'Microsoft YaHei'}.purchaseTable th:nth-child(1),td:nth-child(1){max-width:50px;text-align:left}tbody tr{text-align:center;height:70px;letter-spacing:-.2px;color:#3e3e3e}.purchaseTable img{width:50px;height:50px;border-radius:2px;margin:10px 11px 10px 0;cursor:pointer}.imgRight{letter-spacing:-.3px}