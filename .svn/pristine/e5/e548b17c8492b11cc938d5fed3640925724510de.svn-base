*{padding:0}.nav-height{position:relative;height:50px}.nav-wrap{width:100%;height:50px;background:#fff}.nav-wrap.navFix{position:fixed;top:0;left:0;box-shadow:0 0 5px rgba(0,0,0,.2);border-bottom:1px solid #e3e3e3\9;z-index:99999}.nav-wrap ul{padding:0;margin:0 auto;display:block;border-bottom:3px}.nav-wrap.navFix ul{border-bottom:0}.nav-wrap li{display:inline-block;text-align:left;height:50px;line-height:50px;float:left}.nav-wrap li a{display:block;padding:0 20px;font-size:15px;color:#333;text-decoration:none}.nav-wrap li a:hover{color:#4680d1}.nav-wrap li a.active{border-bottom:2px solid #4680d1;color:#4680d1}.nav-mobile{display:none;font-weight:700;width:100%}.click-me{width:150px;height:30px;background:#4680d1;color:#fff;text-align:center;line-height:30px}.click-me a{display:block;color:#fff}h4,h5{font-family:Microsoft Yahei}.nav-li{font-family:'微软雅黑'}.btn.sub{background:#3A7BD5;border:none}.table thead tr th.th_partner{border-bottom:0;color:#C1C1C1;font-weight:400}.info-white{color:#fff;padding:10px 0}.info-gray{color:#C1C1C1;padding:10px 0}.info-team{margin-left:auto;margin-right:auto;margin-top:10px}.info-product{max-width:100%;padding:0 10px;color:#8c8c8c;font-size:.875em}p{color:#C1C1C1}.p-title{color:#28292D;font-size:2.25em;font-weight:700;padding-top:2.5em}.product{width:100%}.img-product{width:8.125em;height:7.625em}.info-process{font-size:.78125em;color:#8c8c8c}.step-process{color:#606060;font-weight:700;font-size:1em}*,.swiper-slide,.swiper-slide div{margin:0}